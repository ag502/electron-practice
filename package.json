{
  "name": "Readit",
  "version": "0.9.0",
  "description": "Master Electron Project",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec electron .",
    "build": "electron-builder --help",
    "win-cert": "electron-builder create-self-signed-cert -p lee",
    "release": "electron-builder -m -p 'onTagOrDraft'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ag502/electron-practice.git"
  },
  "keywords": [],
  "author": "Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ag502/electron-practice/issues"
  },
  "homepage": "https://github.com/ag502/electron-practice#readme",
  "devDependencies": {
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "nodemon": "^2.0.20"
  },
  "build": {
    "appId": "com.lee.readit",
    "copyright": "Copyright free",
    "publish": {
      "provider": "github"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "zip",
      "certificateFile": "private/lee.p12",
      "certificatePassword": "",
      "verifyUpdateCodeSignature": false,
      "publisherName": "lee"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0"
  }
}
